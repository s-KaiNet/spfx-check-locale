{
  "name": "spfx-check-locale",
  "displayName": "SPFx Check Locale",
  "description": "Dynamically checks the consistency of your localization files inside SharePoint Framework projects, i.e. localization files match the schema inside \"mystrings.d.ts\" .",
  "version": "0.0.1",
  "license": "MIT",
  "keywords": [
    "SharePoint Framework",
    "Microsoft 365",
    "SPFx"
  ],
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "homepage": "https://github.com/s-KaiNet/spfx-check-locale/blob/master/vscode",
  "repository": {
    "url": "https://github.com/s-KaiNet/spfx-check-locale.git",
    "type": "git"
  },
  "publisher": "s-kainet",
  "icon": "assets/icon128.png",
  "activationEvents": [
    "workspaceContains:./config/package-solution.json"
  ],
  "main": "./lib/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm run lint && tsc -p ./",
    "dev": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "spfx-check-locale": "^0.0.3"
  }
}
